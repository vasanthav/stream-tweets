{
  "name": "twitter-stream",
  "version": "1.0.0",
  "description": "Stream and query tweets",
  "scripts": {
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
